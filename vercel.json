{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "installCommand": "pnpm install",
  "buildCommand": "pnpm --filter @petmedi/database exec prisma generate && pnpm --filter @petmedi/web build"
}
