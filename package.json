{
  "name": "petmedi",
  "version": "0.1.0",
  "private": true,
  "description": "동물병원 EMR/PMS SaaS 플랫폼",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "turbo run typecheck",
    "db:generate": "turbo run db:generate --filter=@petmedi/database",
    "db:push": "turbo run db:push --filter=@petmedi/database",
    "db:migrate": "turbo run db:migrate --filter=@petmedi/database",
    "db:studio": "turbo run db:studio --filter=@petmedi/database"
  },
  "keywords": [
    "veterinary",
    "emr",
    "pms",
    "saas",
    "동물병원"
  ],
  "author": "",
  "license": "UNLICENSED",
  "packageManager": "pnpm@10.24.0",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.48.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "prettier": "^3.7.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3"
  }
}
